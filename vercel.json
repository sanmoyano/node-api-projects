{
  "version":2,
  "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
  "routes":[
      {
        "src":"/(.*)",
        "dest":"/"
      },
      {
        "src":"/api/*",
        "dest":"https://node-api-projects.vercel.app/api/projects"
      } 
    ]
}